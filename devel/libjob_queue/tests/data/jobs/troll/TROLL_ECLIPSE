
-- Redirecting - the stdin is essential - the rest are not.
STDIN    eclipse.stdin
STDOUT   eclipse.stdout
STDERR   eclipse.stderr

INIT_CODE   cond_symlink('/prog/ecl/grid/macros/CONFIG.ECL','CONFIG.ECL')
INIT_CODE   cond_unlink('<SMSPEC>')

PLATFORM_EXE  x86_64  /prog/ecl/grid/2007.2/bin/linux_x86_64/eclipse.exe  
PLATFORM_EXE  i686    /prog/ecl/grid/2007.2/bin/linux/eclipse.exe

LSF_RESOURCES    select[x86_64Linux] rusage[ecl100v2000=1:duration=5]
ENV         	 F_UFMTENDIAN     big
ENV         	 LM_LICENSE_FILE  7321@st-linlic01.st.statoil.no,7321@st-linlic02.st.statoil.no,7321@st-linlic03.st.statoil.no

